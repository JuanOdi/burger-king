                                each menuItem in menuData
                                    .top-menu__swiper02-slide
                                        a.top-menu__swiper02-link(href="#")
                                            .top-menu__swiper02-img.img
                                                img.img__main(src=`${assetsPath}/images/top/`+ menuItem.img, alt="")
                                            p.top-menu__swiper02-date #{menuItem.date}
                                            h2.top-menu__swiper02-ttl !{menuItem.ttl}
                                            .top-menu__swiper02-wrap
                                                p.top-menu__swiper02-cat #{menuItem.cat}
